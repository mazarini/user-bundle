{
    "name": "mazarini/user-bundle",
    "description": "Symfony bundle to manage users",
    "type": "symfony-bundle",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.x-dev",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "1.16.x-dev",
        "symfony/framework-bundle": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/yaml": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "3.x-dev",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "6.0.x-dev",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Mazarini\\UserBundle\\": "lib/UserBundle/lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Mazarini\\UserBundle\\Tests\\": "lib/UserBundle/tests"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "authors": [
        {
            "name": "Mazarini",
            "email": "mazarini@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
